{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./ts-dist/",
    "typeRoots": [
      "../../node_modules/@types",
      "node_modules/@types",
      "./src/@types"
    ],
    "types": ["webpack-env", "jest", "resize-observer-browser"],
    "lib": ["dom", "ESNEXT", "DOM.Iterable", "WebWorker.ImportScripts"],
    "experimentalDecorators": true,
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx"
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
